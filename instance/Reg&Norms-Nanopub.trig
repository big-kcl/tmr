        vocab:partOf :CIG-DB ;
        vocab:aboutExecutionOf :ActAdministerDipyridamole ; #:ActAdministerTramadol; Changed for HealthINF Extension at Springer
        vocab:basedOn :CBDipyridamoleThrombus_assertion . #:CBTramadolCoagulation_assertion .
}

